package q01.quiz01;

import java.util.Scanner;

public class quiz05 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		System.out.println("2개의 정수를 입력하세요. ");
		int num1,num2 = input.nextInt();
		
		int a = num1;
		int b = num2;
		
		int result1 = a + b;
		System.out.println(result1);
		
		
		
		
		
	}

}

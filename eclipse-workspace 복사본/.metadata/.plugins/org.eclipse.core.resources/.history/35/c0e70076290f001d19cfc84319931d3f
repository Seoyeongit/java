package q01.quiz01;

import java.util.Scanner;

public class quiz05 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		System.out.println("2개의 정수를 입력하세요. ");
		int a= input.nextInt();
		
		System.out.println(a);
		
		int b= input.nextInt();
		System.out.println (b);
		
		int result1 = a + b;
		System.out.println(result1);
		
		
		
		
		
		
		
		
		
	}

}

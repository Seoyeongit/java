package q01.quiz01;

public class quiz02 {

	public static void main(String[] args) {
		int minutes = 123000;
		int day = 24 * 60;
		int year = 365 * 1440;
		
		int result = minutes / day;
		System.out.println(result + "일");
		
		int result2 = minutes / year;
		System.out.println(result2 + "년");
		
		
		
		
	}

}

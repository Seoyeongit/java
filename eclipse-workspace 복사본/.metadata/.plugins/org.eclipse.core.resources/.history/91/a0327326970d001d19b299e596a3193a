package p01.basic;

public class ArithmeticOperatorEx {

	public static void main(String[] args) {
		int a1 = 10;
		int a2 = 3;
		int result;
		double dresult;
		
		result = a1 + a2*5;
		System.out.println("result = " + result);
		
		result = a1 - a2;
		System.out.println("result = " + result);
		
		result = a1 * a2;
		System.out.println("result = " + result);
		
		result = a1 / a2;
		System.out.println("result = " + result);
		
		result = a1 % a2;
		System.out.println("result = " + result);

	}

}
